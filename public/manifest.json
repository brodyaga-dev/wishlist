{
	"manifest_version": 2,
	"name": "The last wishlist",
	"description": "The latest wishlist is an easy way to visualize your upcoming purchases. No advertising or tampering with your privacy. Just your wishlist. Make your dream come true! ",
	"version": "1.2.2",
	"permissions": ["storage", "tabs"],
	"icons": { "16": "icons/icon16.png", "24": "icons/icon24.png", "32": "icons/icon32.png" },
	"browser_action": {
		"default_icon": {
			"16": "icons/icon16.png",
			"24": "icons/icon24.png",
			"32": "icons/icon32.png"
		},
		"default_popup": "popup.html",
		"default_title": "The last wishlist"
	},
	"applications": {
		"gecko": {
			"id": "denisne1ru@gmail.com",
			"strict_min_version": "80.0"
		}
	}
}
